{
  "$schema": "http://json-schema.org/schema#",
  "$id": "https://smart-data-models.github.io/datamodel.AutonomousMobileRobot/EmergencyCommand/ReturnMessage/schema.json",
  "type": "object",
  "properties": {
    "type": {
      "type": "object",
      "description": "Relationship. NGSI Entity type. It has to be AutonomousMobileRobot",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json#/definitions/type"
    },
    "time": {
      "type": "object",
      "description": "Relationship. The time when the command was sent.",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json#/definitions/time"
    },
    "receivedTime": {
      "type": "object",
      "description": "Relationship. The time when the robot received the stop command.",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json/schema.json#/definitions/time"
    },
    "receivedStopCommand": {
      "type": "object",
      "description": "Relationship. The stop command which the robot received.",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json/schema.json#/definitions/stopCommand"
    },
    "result": {
      "type": "object",
      "description": "Relationship. The result of the robot received the stop command.",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json/schema.json#/definitions/resultsOfStopCommand"
    },
    "errors": {
      "type": "object",
      "description": "Relationship. Describes the errors that occurred in the robot.",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json/schema.json#/definitions/errors"
    }
  },
  "required": [
    "id",
    "type",
    "time",
    "receivedTime",
    "receivedStopCommand",
    "result",
    "errors"
  ],
  "additionalProperties": true
}
